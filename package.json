{
  "name": "yarrow",
  "version": "0.1.0",
  "description": "Shopify theme for Victoria's Mobile Flower Shop",
  "main": "index.js",
  "scripts": {
    "deploy:dev": "shopify-themekit deploy --vars=.env --env=development",
